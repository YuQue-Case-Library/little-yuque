@import "/static/style/common.wxss";

.container {
  width: 750rpx;
}

.header {
  box-sizing: border-box;
  padding-left: 24rpx;
  width: 100%;
  height: 100rpx;
  border-bottom: 1px solid #e8e8e8;
}

.header .header-center {
  width: 160rpx;
  height: 64rpx;
}

.header .header-right .text {
  margin-right: 16rpx;
  font-size: 24rpx;
  color: #595959;
  white-space: nowrap;
}