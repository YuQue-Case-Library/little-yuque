@font-face {
  font-family: 'm-larkicon';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}

.m-larkicon:before {
  font-family: m-larkicon,sans-serif
}

.m-larkicon-close:before {
  content: "\E60A"
}

.m-larkicon-toc:before {
  content: "\E60C"
}

.m-larkicon-arrow-left:before {
  content: "\E60D"
}

